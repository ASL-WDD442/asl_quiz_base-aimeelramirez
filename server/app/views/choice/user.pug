extends ../_layout
include ../_includes/link_mixin
block content
    h1.heading My Choices
    ul.list
        for choice in choices
            li.list__item
                span.list__item__title= choice.title
                +link('/admin/choice/'+choice.id, 'View', 'fa-eye')
    +link('/admin/quizzes/new', 'Create Your Own Quiz', '', 'button primary')